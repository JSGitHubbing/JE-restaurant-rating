 {
  "manifest_version": 2,
  "name": "je-restaurant-manager",
  "version": "0.1",

  "icons": {
    "64": "icons/stock.png"
  },

  "content_scripts": [
    {
      "matches": ["https://www.just-eat.es/area/*"],
      "js": ["src/borderify.js"],
	  "css": ["src/modal.css"]
    }
  ],

  "permissions": ["storage"],
  "web_accessible_resources": ["icons/badplace.png", "icons/stock.png", "src/modal.html"]
 }